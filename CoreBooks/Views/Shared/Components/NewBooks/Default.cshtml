@model IEnumerable<CoreBooks.Models.Books>
 
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <td>BookName</td>
        <td>Author</td>
    </tr>
    @foreach (var k in Model)
    {
        <tr>
            <td>@k.ID</td>
            <td>@k.BookName</td>
            <td>@k.Author</td>
        </tr>
    }
</table>
